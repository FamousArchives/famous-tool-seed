  },
  "author": "Andrew de Andrade <andrew@famo.us>",
  "devDependencies": {
    "eslint": "^0.8.2",
    "nodemon": "^1.2.1",
    "tap": "^0.4.12"
  },
  "dependencies": {
    "minimist": "^1.1.0"
  },
  "bin": {
    "famous-tool-seed": "bin/cmd.js"
  },
  "preferGlobal": "true",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "readme": "README.markdown",
  "engines": {
    "node": ">=0.10.28",
    "npm": ">=1.4.10"
  },
