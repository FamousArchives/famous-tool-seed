{
  "name": "famous-node-module",
  "version": "0.0.0",
  "description": "Base Famo.us NodeJS Module",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/famous/famous-node-module.git"
  },
  "scripts": {
    "test": "npm run-script lint; ./node_modules/.bin/tap test/*.js",
    "watch": "./node_modules/.bin/nodemon --ignore node_modules/ -e js,json --exec 'npm test'",
    "lint": "./node_modules/.bin/eslint **/*.js"
  },
  "author": "Andrew de Andrade <andrew@famo.us>",
  "devDependencies": {
    "eslint": "^0.6.2",
    "nodemon": "^1.2.0",
    "tap": "^0.4.11",
    "tape": "^2.13.2"
  },
  "dependencies": {
    "minimist": "^0.1.0"
  },
  "bin": {
    "famous-command": "bin/cmd.js"
  },
  "preferGlobal": "true",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "readme": "README.markdown",
  "engines": {
    "node": ">=0.10.28",
    "npm": ">=1.4.10"
  },
  "license": "TBD"
}
